{
  "name": "trash",
  "version": "9.0.0",
  "description": "Move files and folders to the trash",
  "license": "MIT",
  "repository": "sindresorhus/trash",
  "funding": "https://github.com/sponsors/sindresorhus",
  "author": {
    "name": "Sindre Sorhus",
    "email": "sindresorhus@gmail.com",
    "url": "https://sindresorhus.com"
  },
  "type": "module",
  "exports": {
    "types": "./index.d.ts",
    "default": "./index.js"
  },
  "sideEffects": false,
  "engines": {
