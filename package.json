    "files",
    "directory",
    "directories",
    "folder",
    "folders",
    "xdg"
  ],
  "devDependencies": {
    "ava": "^6.1.3",
    "tempfile": "^5.0.0",
    "tsd": "^0.31.1",
    "typescript": "^5.5.4",
    "xo": "^0.59.2"
  },
  "ava": {
    "workerThreads": false
  }
